{
    "/hospital/*": {
        "target": "https://hospitalapiserver.azurewebsites.net",
        "secure": true,
        "pathRewrite": {
            "^/hospital": "/api"
        },
        "changeOrigin": true,
        "logLevel": "debug"
    },
   "/integration/*": {
        "target": "https://integrationapiserver.azurewebsites.net",
        "secure": true,
